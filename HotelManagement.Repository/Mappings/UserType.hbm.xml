<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="HotelManagement.ServiceApp" assembly="HotelManagement.Models">

  <class name="UserType" table="UserType">

    <id name="Id">
      <column name="UserTypeId" sql-type="int" not-null="true"></column>
      <generator class="identity"></generator>
    </id>

    <property name="Name">
      <column name="Name" not-null="true"></column>
    </property>    

  </class>

</hibernate-mapping>