<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="HotelManagement.Models" assembly="HotelManagement.Models">

  <class name="RoomType" table="RoomType">

    <id name="Id">
      <column name="RoomTypeId" sql-type="int" not-null="true"></column>
      <generator class="identity"></generator>
    </id>

    <property name="Name">
      <column name="Name" not-null="true"></column>
    </property>

    <property name="MaxOccupants">
      <column name="MaxOccupants" not-null="true"></column>
    </property>
    
  </class>

</hibernate-mapping>
