<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="HotelManagement.Models" assembly="HotelManagement">

  <class name="HotelChain" table="HotelChain">

    <id name="Id">
      <column name="HotelChainId" sql-type="int" not-null="true"></column>
      <generator class="identity"></generator>
    </id>

    <property name="Name">
      <column name="HotelChainName" not-null="true"></column>
    </property>    
    
  </class>
  
</hibernate-mapping>